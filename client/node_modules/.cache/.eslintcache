ocuments/Personal_Coding_Projects/Personal-Blog/client/src/App.js",[],[],"/Users/sebastiantischner/Documents/Personal_Coding_Projects/Personal-Blog/client/src/pages/Write.jsx",["59"],[],"import React, { useState } from 'react';\n\nimport ReactQuill from 'react-quill';\nimport 'react-quill/dist/quill.snow.css';\nimport axios from 'axios';\nimport { Navigate, useLocation, useNavigate } from 'react-router-dom';\nimport moment from 'moment';\n\n\nconst Write = () => {\n\n    const state = useLocation().state\n\n    const [value, setValue] = useState(state?.title || \"\");\n    const [title, setTitle] = useState(state?.desc || \"\");\n    const [file, setFile] = useState(null);\n    const [cat, setCat] = useState(state?.cat || \"\");\n\n    const navigate = useNavigate();\n\n    const upload = async () => {\n\n        try {\n            const formData = new FormData();\n            formData.append('file', file)\n            const res = await axios.post('/upload', formData)\n            return res.data\n        } catch (err) {\n            console.log(err)\n        }\n    }\n\n    const handleClick = async e => {\n        e.preventDefault()\n        const imgUrl = await upload();\n\n        try {\n            state ? await axios.put(`/posts/${state.id}`, {\n                title, desc: value, cat, img: file ? imgUrl : \"\"\n            }) : await axios.post(`/posts/`, {\n                title,\n                 desc: value, \n                 cat,\n                  img: file ? \n                  imgUrl : \"\", \n                  date: moment(Date.now()).format(\"YYYY-MM-DD HH:mm:ss\")\n            });\n            navigate('/')\n        } catch (err) {\n            console.log(err)\n        }\n    }\n\n    const getText = (html) => {\n        const doc = new DOMParser().parseFromString(html, \"text/html\")\n        return doc.body.textContent\n    }\n\n    return (\n        <div className='add'>\n            <div className='content'>\n                <input type=\"text\" value={getText(title)} placeholder='Title' onChange={e => setTitle(e.target.value)} />\n                <div className=\"editorContainer\">\n                    <ReactQuill className=\"editor\" theme=\"snow\" placeholder='Blog Body' value={value} onChange={setValue} />\n                </div>\n            </div>\n            <div className='menu'>\n                <div className=\"item\">\n                    <h1>Publish</h1>\n                    {/* <span>\n                        <b>Status: </b> Draft\n                    </span>\n                    <span>\n                        <b>Visibility: </b> Public\n                    </span> */}\n                    \n                    {/* <label className='file' htmlFor=\"file\">Upload Image</label> */}\n                    <input type=\"file\" id=\"file\" name=\"\" className='choose-file' onChange={e => setFile(e.target.files[0])} />\n                    <div></div>\n                    <div className='buttons'>\n                        {/* <button>Save as a draft</button> */}\n                        <button onClick={handleClick}>Publish Blog</button>\n                    </div>\n                </div>\n                <div className=\"item\">\n                    <h1>Category</h1>\n                    <div className=\"cat\">\n                        <input type=\"radio\" checked={cat === \"art\"} name=\"cat\" value=\"art\" id=\"art\" onChange={e => setCat(e.target.value)} />\n                        <label htmlFor=\"art\">ART</label>\n                    </div>\n                    <div className=\"cat\">\n                        <input type=\"radio\" checked={cat === \"food\"} name=\"cat\" value=\"food\" id=\"food\" onChange={e => setCat(e.target.value)} />\n                        <label htmlFor=\"art\">Food</label>\n                    </div>\n                    <div className=\"cat\">\n                        <input type=\"radio\" checked={cat === \"science\"} name=\"cat\" value=\"science\" id=\"science\" onChange={e => setCat(e.target.value)} />\n                        <label htmlFor=\"art\">Science</label>\n                    </div>\n                    <div className=\"cat\">\n                        <input type=\"radio\" checked={cat === \"technology\"} name=\"cat\" value=\"technology\" id=\"design\" onChange={e => setCat(e.target.value)} />\n                        <label htmlFor=\"art\">Technology</label>\n                    </div>\n                    <div className=\"cat\">\n                        <input type=\"radio\" checked={cat === \"design\"} name=\"cat\" value=\"design\" id=\"design\" onChange={e => setCat(e.target.value)} />\n                        <label htmlFor=\"art\">Design</label>\n                    </div>\n                    <div className=\"cat\">\n                        <input type=\"radio\" name=\"cat\" checked={cat === \"cinema\"} value=\"cinema\" id=\"cinema\" onChange={e => setCat(e.target.value)} />\n                        <label htmlFor=\"art\">Cinema</label>\n                    </div>\n                </div>\n            </div>\n        </div>\n    )\n}\n\nexport default Write","/Users/sebastiantischner/Documents/Personal_Coding_Projects/Personal-Blog/client/src/pages/Register.jsx",[],[],"/Users/sebastiantischner/Documents/Personal_Coding_Projects/Personal-Blog/client/src/pages/Login.jsx",[],[],"/Users/sebastiantischner/Documents/Personal_Coding_Projects/Personal-Blog/client/src/pages/Single.jsx",[],[],"/Users/sebastiantischner/Documents/Personal_Coding_Projects/Personal-Blog/client/src/pages/Home.jsx",[],[],"/Users/sebastiantischner/Documents/Personal_Coding_Projects/Personal-Blog/client/src/components/Navbar.jsx",["60"],[],"/Users/sebastiantischner/Documents/Personal_Coding_Projects/Personal-Blog/client/src/components/Footer.jsx",["61"],[],"/Users/sebastiantischner/Documents/Personal_Coding_Projects/Personal-Blog/client/src/components/Menu.jsx",[],[],"/Users/sebastiantischner/Documents/Personal_Coding_Projects/Personal-Blog